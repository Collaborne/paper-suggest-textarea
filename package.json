    "@polymer/iron-component-page": "^4.0.0",
    "@polymer/iron-demo-helpers": "^3.0.0",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "chai": "^4.2.0",
    "mocha": "^6.1.4",
    "polymer-cli": "^1.9.5",
    "ts-node": "^8.0.3",
    "tslint": "^5.14.0",
    "tslint-config-collaborne": "^0.0.4",
    "typescript": "^3.3.4000"
